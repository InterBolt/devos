{
  "name": "solos",
  "displayName": "SolOS Snippets",
  "description": "Snippets for all things SolOS",
  "version": "0.0.1",
  "publisher": "solos",
  "private": true,
  "engines": {
    "vscode": "^1.87.2"
  },
  "activationEvents": [
    "*"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "configuration": [],
    "commands": [
      {
        "command": "solos.cdNaked",
        "title": "Cd Naked",
        "category": "solos"
      }
    ]
  },
  "scripts": {
    "build": "npm run s:compile && npm run s:copy && npm run s:vscode-package",
    "install": "code --uninstall-extension solos.solos || true && code --install-extension ./solos-0.0.1.vsix --force",
    "sync": "npm run build && npm run install",
    "s:compile": "npm install && node ./build.js && npx tsc",
    "s:copy": "cp package.json out/package.json",
    "s:vscode-package": "vsce package --allow-missing-repository --allow-star-activation"
  },
  "devDependencies": {
    "@types/node": "^16.18.34",
    "@types/vscode": "^1.73.0",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "eslint": "^8.26.0",
    "typescript": "^5.4.2"
  }
}